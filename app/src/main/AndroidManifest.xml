<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bujimuapp">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera2" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.home.FarmDetailActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.bujimuapp.ui.MainActivity" />
        </activity>
        <activity android:name=".MaizeAgroActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.bujimuapp.ui.MainActivity" />
        </activity>
        <activity android:name=".BeanAgroActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.bujimuapp.ui.MainActivity" />
        </activity>
        <activity android:name=".PhCameraActivity" />
        <activity android:name=".PhosphorousCameraActivity" />
        <activity android:name=".NitrogenCameraActivity" />
        <activity android:name=".SoilAnalysisActivity" />
        <activity android:name=".ui.SoilActivity"/>
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>